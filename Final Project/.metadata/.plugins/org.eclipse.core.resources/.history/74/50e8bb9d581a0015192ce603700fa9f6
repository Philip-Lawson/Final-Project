package finalproject.poc.classloading;

public enum ClassRequest {
	LOAD_CALCULATOR_CLASS(0), LOAD_WORK_PACKET_CLASS, LOAD_RESULT_PACKET_CLASS;
	
	private int requestNum;
	
	private ClassRequest(int requestNum){
		this.requestNum = requestNum;
	}
	
	public int getRequestNum(){
		return requestNum;
	}

}
